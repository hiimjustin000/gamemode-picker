{
	"geode": "3.9.0",
	"gd": {
		"win": "2.206",
		"android": "2.206",
		"mac": "2.206"
	},
	"version": "v1.0.5",
	"id": "capeling.gamemode_picker",
	"name": "Gamemode Swapper",
	"developer": "Capeling",
	"description": "Adds a button in the Pause Menu that allows you to change your gamemode!",
	"repository": "https://github.com/Capeling/gamemode-picker",
	"tags": [
		"gameplay"
	],
	"dependencies": [
        {
            "id": "geode.node-ids",
            "version": "v1.6.0",
            "importance": "required"
        },
		{
			"id": "geode.custom-keybinds",
			"version": "v1.5.0",
			"importance": "required",
			"platforms": ["win", "android"]
		}
	],
	"settings": {
		"topRight": {
			"type": "bool",
			"default": false,
			"name": "Move Button",
			"description": "Moves the button to the top right and changes the texture to the list edit button"
		}
	},
	"resources": {
		"spritesheets": {
			"GamemodeSheet":[
				"resources/GamemodeSheet/unused/*.png",
				"resources/GamemodeSheet/speeds/*.png",
				"resources/GamemodeSheet/player/*.png"
			]
		}
	}
}
